# container-specific Oh My Zsh settings
export ZSH="$HOME/.oh-my-zsh"

ZSH_THEME="agnoster"
plugins=(git)

zstyle ':omz:update' mode auto

HIST_STAMPS="yyyy/mm/dd"

alias rbz="source $HOME/.zshrc"
alias synczsh='cp /workspace/.devcontainer/cloudrun/zshrc.cloudrun ~/.zshrc && source ~/.zshrc'

alias lsa='exa -la'

alias run='python backend/run.py'

export PATH=$PATH:/usr/local/go/bin

source $HOME/.oh-my-zsh/oh-my-zsh.sh